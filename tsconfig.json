{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"strictNullChecks": true,
		"baseUrl": ".",
		"paths": {
			"@content/*": [
				"src/content/*"
			],
			"@components/*": [
				"src/components/*"
			],
			"@lib/*": [
				"src/lib/*"
			],
			"@layouts/*": [
				"src/layouts/*"
			],
			"@consts": [
				"src/consts.ts"
			]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "preact"
	}
}
